{
  "name": "@keqingrong/hooks",
  "version": "0.1.0",
  "description": "Collection of React Hooks",
  "author": "Qingrong Ke <keqingrong@outlook.com> (https://keqingrong.github.io/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keqingrong/hooks.git"
  },
  "main": "dist/index.js",
  "module": "dist/hooks.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "keywords": [
    "react",
    "hooks"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build --format cjs,esm,umd",
    "serve": "npx serve public",
    "test": "tsdx test --testPathIgnorePatterns example",
    "lint": "tsdx lint src test",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "qs": "^6.10.1",
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.1",
    "@types/qs": "^6.9.6",
    "@types/react": "^17.0.4",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint-config-react-app": "^6.0.0",
    "husky": "^4.3.6",
    "qs": "^6.10.1",
    "react": "^17.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "size-limit": "^4.9.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "resolutions": {
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "eslint-config-react-app": "6.0.0",
    "rollup-plugin-typescript2": "0.30.0",
    "tslib": "2.2.0",
    "typescript": "4.2.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "size-limit": [
    {
      "path": "dist/hooks.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/hooks.esm.js",
      "limit": "10 KB"
    }
  ]
}
